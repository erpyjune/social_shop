<script type="text/javascript">
var Footer_toTheTopEnable = false;
$(document).ready(function () {
    if ("N" == "Y") {
        $("#viewport").attr("content", "user-scalable=no, initial-scale=1, width=320"); // IPAD iOs7 에서 안먹어서 추가
    }

    $(".policy-company_item").bind('click', function () {
        if ($(this).attr('url') == "") { return false; }

        cLog.go('CMB400');
        if ($(this).attr('blankYn') == "Y") {
            window.open($(this).attr('url'));
        } else {
            self.location.href = $(this).attr('url');
        }
    });

    //  맨위로 버튼이 없으면 추가한다.
    if ($("#toTheTop").length <= 0 && location.href.toLowerCase().indexOf(_CONST_MOBILEVIPURL.toLowerCase()) < 0) {
        if ($("div.l-wrapper").length > 0) {
            var toTheTopHtml = "<a href='#' class='to-the-top' id='toTheTop'><strong>맨위로</strong></a>";
            $("div.l-wrapper").append(toTheTopHtml);
        }
    }

    cLog = new CLog("http://mobile.auction.co.kr/CLog.aspx");   // 헤더/푸터에서 2번 생성 ( Footer에서 2nd 생성) 

});


</script>
