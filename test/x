	$(document).ready(function () {
		var homepage = Header.CheckHomePage();
		var checkIssue = "원터치 모기장 ";
		var checkIssueMobile = "http://mobile.auction.co.kr/Search/List.aspx?category=28440100";
		if (homepage == true && checkIssue.length > 0 && checkIssueMobile.length > 0) {
			Header.SetIssueKeyword(checkIssue, checkIssueMobile);
		} else {
			$('#keyword').val("");
		}
	});
